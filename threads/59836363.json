        "user_id": 6782896,
        "user_type": "registered",
        "accept_rate": 62,
        "profile_image": "https://www.gravatar.com/avatar/343a61ec30951aa0f3f0ba3bf94fbb58?s=256&d=identicon&r=PG&f=y&so-version=2",
        "display_name": "Jashanpreet Singh",
